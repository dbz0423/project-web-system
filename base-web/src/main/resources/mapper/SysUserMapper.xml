<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.zhu.web.sys_user.mapper.SysUserMapper">
    <select id="selectById" resultType="top.zhu.web.sys_user.entity.SysUser">
        SELECT * FROM sys_user AS ur WHERE ur.user_id = #{userId}
    </select>
</mapper>